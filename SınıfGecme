import java.util.Scanner;

public class SınıfGecme {
    public static void main(String[] args) {
        /*
        Dersler : Matematik, Fizik, Türkçe, Kimya, Müzik
        Geçme Notu : 55
        Ödev
        Eğer girilen ders notları 0 veya 100 arasında değil ise ortalamaya katılmasın.
         */
        int fizik,kimya,matematik,türkce,müzik,sayıcı=0;
        double ortalama=0,totalOrtalama = 0;
        Scanner input=new Scanner(System.in);
        System.out.println("Lütfen fizik notunuzu giriniz : ");
        fizik=input.nextInt();
        if (fizik<100 && fizik>0){
            ortalama+=fizik;
            sayıcı++;

        }
        System.out.println("Lütfen kimya notunuzu giriniz : ");
        kimya=input.nextInt();
        if (kimya<100 && kimya>0){
            ortalama+=kimya;
            sayıcı++;

        }
        
        System.out.println("Lütfen matematik notunuzu giriniz : ");
        matematik=input.nextInt();
        if (matematik<100 && matematik>0){
            ortalama+=matematik;
            sayıcı++;

        }
        
        System.out.println("Lütfen türkce notunuzu giriniz : ");
        türkce=input.nextInt();
        if (türkce<100 && türkce>0){
            ortalama+=türkce;
            sayıcı++;

        }
        
        System.out.println("Lütfen müzik notunuzu giriniz : ");
        müzik=input.nextInt();if (müzik<100 && müzik>0){
            ortalama+=müzik;
            sayıcı++;

        }
        totalOrtalama=ortalama/sayıcı;
        System.out.println("Ortalama : "+totalOrtalama);
        



    }
}
